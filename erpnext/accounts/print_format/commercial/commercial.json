 "html": "{{letter_head}}\n<div>\n\t\t<h3>Commercial Invoice</h3>\n\t\t<h4 class=\"text-muted\">{{ doc.ref_no or \"\" }}</h4>\n\t</div>\n<hr style=\"height:3px;border:none;color:#ddd;background-color:#ddd;\" />\n\n\n<div class=\"row\">\n\t<div class=\"col-xs-6\">\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><b>Date:</b></div>\n\t\t\t<div class=\"col-xs-7 text-left\">{{  frappe.utils.formatdate(doc.posting_date)  }}</div>\n\t\t</div>\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><b>Date:</b></div>\n\t\t\t<div class=\"col-xs-7 text-left\">{{  frappe.utils.formatdate(doc.due_date)  }}</div>\n\t\t</div>\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><label>Date Of Delivery:</label></div>\n\t\t\t<div class=\"col-xs-7 text-left\">{{ frappe.utils.formatdate(doc.delivery_date) }}</div>\n\t\t</div>\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><label>Mode Of Delivery:</label></div>\n\t\t\t<div class=\"col-xs-7 text-left\">{{  doc.mode_of_delivery or ''}}</div>\n\t\t</div>\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><label>Terms Of Delivery:</label></div>\n\t\t\t<div class=\"col-xs-7 text-left\">{{  doc.terms_of_delivery or ''}}</div>\n\t\t</div>\n\t</div>\n\n\t<div class=\"col-xs-6\">\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><label>Customer Name :</label></div>\n\t\t\t<div class=\"col-xs-7 text-left\"> {{  frappe.db.get_value('Customer',{'name':doc.customer},'customer_name') or ''}}</div>\n\t\t</div>\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><label>Contact Person:</label></div>\n                        {% if doc.contact_person -%}\n\t\t\t\t<div class=\"col-xs-7\">{{ frappe.db.get_value('Contact',doc.contact_person,'first_name') + ' ' + frappe.db.get_value('Contact',doc.contact_person,'last_name') + '<br>Phone: ' + (frappe.db.get_value('Contact',doc.contact_person,'phone') or '')}}</div>\n                        {% else %}\n                                <div class=\"col-xs-7\"> </div>\n\t\t        {% endif %}\n\t\t\t\n\t\t</div>\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-5 text-left\"><label>Billing Address :</label></div>\n\t\t\t<div class=\"col-xs-7 text-left\">{{ doc.address_display or ''}}</div>\n\t\t</div>\n\t</div>\n</div>\n\n<table class=\"table table-bordered\">\n<tr>\n<th style=\"width:5%\" >Sr</th>\n<th style=\"width:30%\" >Item</th>\n<th style=\"width:5%\" >Grade</th>\n<th style=\"width:15%\" >Qty</th>\n<th style=\"width:20%\" >Rate</th>\n<th style=\"width:25%\" >Amount</th>\n</tr>\n{% set count=1 %}\n{%- for row in doc.entries -%}\n<tr>\n<td style=\"width:5%\" >{{ count }}</td>\n<td style=\"width:30%\" >{{ row.item_name }}</td>\n<td style=\"width:5%\" >{{ row.grade }}</td>\n<td style=\"width:15%\" >{{ row.qty }}<p>{{ row.stock_uom }}</p></td>\n<td style=\"width:20%\" >{{ row.get_formatted(\"rate\", doc) }}</td>\n<td style=\"width:25%\" >{{ row.get_formatted(\"amount\", doc) }}</td>\n</tr>\n{% set count=count+1 %}\n\n{%- endfor %}\n</table>\n<table style=\"width:100%\">\n<tr style=\"width:100%\">\n<td style=\"width:60%;text-align:right\"><b>Net Total</b></td>\n<td style=\"width:40%;text-align:right\">{{ doc.get_formatted(\"net_total_export\") }}</td>\n</tr>\n\n{%- for row in doc.other_charges -%}\n<tr  style=\"width:100%\">\n<td style=\"width:40%;text-align:right\"><b>{{row.description}}</b></td>\n<td style=\"width:60%;text-align:right\">{{frappe.db.get_value('Currency', doc.currency, 'symbol')}} {{ \"%.2f\"%((row.tax_amount) / (doc.conversion_rate))}}</td>\n</tr>\n{%- endfor -%}\n<tr style=\"width:100%\">\n<td style=\"width:60%;text-align:right\"><b>Grand Total</b></td>\n<td style=\"width:40%;text-align:right\">{{ doc.get_formatted(\"grand_total_export\") }}</td>\n</tr>\n</table>\n</br>\n\n{% if doc.terms -%}\n<div class=\"row\" style=\"border:1px solid #ddd;margin-left:5px;margin-right:5px;\">\n  <div class=\"col-xs-12 text-left\" >\n           {{ doc.terms or '' }}\n     </div>\n</div>\n{%- endif %}", 
