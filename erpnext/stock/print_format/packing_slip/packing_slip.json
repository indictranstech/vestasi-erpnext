 "html": "{{letter_head}}\n<div>\n\t\t<h4>Consignment Note</h4> \n       <h4>Packing list No: {{ doc.ref_no or \"\" }}</h4>\n\t</div>\n<hr style=\"height:1px;border:none;color:#333;background-color:#333;\" />\n\n<div class=\"row\">\n<div class=\"col-xs-6\">\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Delivery Note:</b></div>\n<div class=\"col-xs-7 text-left\">{{  doc.delivery_note  }}</div>\n</div>\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Date Of Delivery:</b></div>\n<div class=\"col-xs-7 text-left\">{{ frappe.utils.formatdate(doc.date_of_delivery) }}</div>\n</div>\n\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Mode Of Delivery:</b></div>\n<div class=\"col-xs-7 text-left\">{{  doc.mode_of_delivery or ''}}</div>\n</div>\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Contact Person:</b></div>\n<div class=\"col-xs-7\">{{  doc.contact_person or ''}}</div>\n</div>\n\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Terms Of Delivery:</b></div>\n<div class=\"col-xs-7 text-left\">{{  doc.terms_of_delivery or ''}}</div>\n</div>\n\n\n</div>\n\n\n<div class=\"col-xs-6\">\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Customer PO No. :</b></div>\n<div class=\"col-xs-7 text-left\"> {{ doc.po_no or ''}}</div>\n</div>\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Customer Name :</b></div>\n<div class=\"col-xs-7 text-left\"> {{ frappe.db.get_value('Address',{'name':doc.billing_address},'customer') or ''}}</div>\n</div>\n\n<div class=\"row\">\n<div class=\"col-xs-5 text-left\"><b>Shipping Address :</b></div>\n<div class=\"col-xs-7 text-left\">{{ doc.address_display or ''}}</div>\n</div>\n\n</div>\n\n</div>\n\n\n\n</br>\n\n<table class=\"table table-bordered\"> \n\t<tbody>\n\t\t<tr>\n\t\t\t<th>Sr</th>\n\t\t\t<th>Item Code</th>\n\t\t\t<th>Item Name</th>\n\t\t\t<th class=\"text-right\">Quantity</th>\n\t\t\t<th class=\"text-right\">UOM</th>\n\t\t</tr>\n\t\t{%- for row in doc.item_details -%}\n\t\t<tr>\n\t\t\t<td style=\"width: 3%;\">{{ row.idx }}</td>\n\t\t\t<td style=\"width: 20%;\">\n\t\t\t\t<b>{{ row.item_code }}</b>\n                                  \n                                {% if row.item_code != row.item_name -%}\n\t\t\t\t<br>Item Code: {{ row.item_code}}\n                                 {%- endif %}\n\t\t\t</td>\n\t\t\t<td style=\"width: 20%;\">\n\t\t\t\t<div style=\"border: 0px;\">{{ row.item_name }}</div></td>\n\t\t\t<td style=\"width: 10%; text-align: right;\">{{ row.qty }} </td>\n\t\t\t<td style=\"width: 15%; text-align: right;\">{{\n\t\t\t\trow.uom or row.stock_uom  }}</td>\n\t\t</tr>\n\t\t{%- endfor -%}\n\t</tbody>\n</table>\n</br>\n\n\n<table  class=\"table\" >\n\t<tr style=\"width: 100%;border: 0px;\"><td style=\"width: 50%;border: 0px;\"><b>Net Weight:</b> {{ doc.net_weight_pkg}}</td>\n                       <td style=\"width: 50%;border: 0px;\"><b>Gross Weight:</b>      {{doc.gross_weight_pkg }}</td></tr>\n       <tr style=\"width: 100%;border: 0px;\"><td style=\"width: 50%;border: 0px;\"><b>Net Weight UOM :</b>     {{ doc.net_weight_uom or '' }}</td>\n                      <td style=\"width: 50%;border: 0px;\"><b>Gross Weight UOM:</b>  {{ doc.gross_weight_uom or '' }}</td></tr>\n</table>\n</br>\n</br>\n</br>\n{%- if doc.terms %}\n<div class=\"row\" style=\"border:1px solid #ddd;margin-left:5px;margin-right:5px;\">\n  <div class=\"col-xs-12 text-left\" >\n           {{ doc.terms or '' }}\n     </div>\n</div>\n{%- endif %}\n\n\n\n\n\n\n\n<div>", 
